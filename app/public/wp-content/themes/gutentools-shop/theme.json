							"padding": {
								"bottom": "clamp(0.75rem, 2.5vw, 0.938rem)",
								"left": "clamp(1.563px, 2.5vw, 1.875rem)",
								"right": "clamp(1.563px, 2.5vw, 1.875rem)",
								"top": "clamp(0.75rem, 2.5vw, 0.938rem)"
							}
						},
						"border": {
							"width": "1px"
						}
					}
				},
				"spacing": {
					"padding": {
								"bottom": "clamp(0.75rem, 2.5vw, 0.938rem)",
								"left": "clamp(1.563px, 2.5vw, 1.875rem)",
								"right": "clamp(1.563px, 2.5vw, 1.875rem)",
								"top": "clamp(0.75rem, 2.5vw, 0.938rem)"
							}
				},
			"border": {
				"radius":"50px"
			    },
			"color": {
				"background": "var(--wp--preset--color--primary)",
				"text": "var(--wp--preset--color--button-hover-text-color)"
