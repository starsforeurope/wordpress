				{
					"color": "#1A4870",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#f8f9f8",
					"name": "Tertiary",
					"slug": "tertiary"
				},
				{
					"color": "#F9DBBA",
					"name": "Secondary Background",
					"slug": "secondary-bg"
				},
				{
					"color": "#fff",
					"name": "White",
					"slug": "white"
				},
				{
					"color": "#000",
					"name": "Black",
					"slug": "black"
				},
				{
					"color": "#F4F4F4",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#0C0C0C",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#121212",
					"name": "Dark Text Color",
					"slug": "dark-text-color"
				},
				{
					"color": "#111111",
					"name": "Link Color",
					"slug": "link-color"
				},				
				{
					"color": "#f2f2f2",
					"name": "Button Hover Text Color",
					"slug": "button-hover-text-color"
